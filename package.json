{
  "name": "santosh-ui-components",
  "version": "0.0.1",
  "description": "This santosh-ui-components Componets can be used across many webapps using nodejs",
  "author": "Bommepalli Santosh Kumar Reddy",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "microbundle-crl": "^0.13.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typedoc": "^0.22.18",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build:local": "npm version patch && npm run build",
    "eject": "react-scripts eject",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "cross-env CI=1 react-scripts test --env=jsdom --coverage --watchAll=false",
    "doc": "npx typedoc",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "files": [
    "dist",
    "dist/core",
    "dist/**/*"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
